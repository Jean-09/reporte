{
  "kind": "collectionType",
  "collectionName": "departamentos",
  "info": {
    "singularName": "departamento",
    "pluralName": "departamentos",
    "displayName": "departamento"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "text",
      "maxLength": 100,
      "required": true
    },
    "descripcion": {
      "type": "text"
    },
    "activo": {
      "type": "boolean",
      "default": true
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "departamento"
    }
  }
}
